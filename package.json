{
  "name": "testing-ground",
  "version": "0.0.1",
  "description": "SSR Testing with React 16 && Webpack 4",
  "author": "Joe Dodd",
  "main": "index.js",
  "scripts": {
    "start": "webpack -w --mode development & nodemon server.js",
    "build": "webpack --mode production"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.12.5",
    "serialize-javascript": "^1.4.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  }
}
